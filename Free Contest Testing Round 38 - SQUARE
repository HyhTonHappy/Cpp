#include <bits/stdc++.h>
using namespace std;
#define ll long long
ll n, k;
ll a[200005];
int main() {
   cin >> n >> k;
   for(int i = 1; i<= n - k;i++){
    for(int j = 1; j<= n; j++){
        cout << "*" << " ";
    }
    cout << endl;
   }
   for(int i = 1; i<=k;i++){
    for(int j = 1; j <= n - k; j++){
        cout << "*" << ' ';
    }
    cout << endl;
   }
    return 0;
}
